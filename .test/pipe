echo beep | echo boop;

echo beep | cat -e;

echo oiIOiOioioioiioIO | tr I _ | tr '[:lower:]' '[:upper:]';
echo oiIOiOioioioiioIO | tr '[:lower:]' '[:upper:]' | tr I _;

echo beep | cat; echo $?
echo beep | ls donk; echo $?
exit 1 | exit 2 | exit 3; echo $?

cat /dev/urandom | head -c 100 > urandom.100.log;
cat /dev/urandom | head -c 200 > urandom.200.log;
