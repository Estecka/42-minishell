echo "$BEEP"boop; echo $BEEP'boop'; echo '$BEEP boop';
echo -n a b c d;
echo -nn a b c d;
echo a -n b c d;

echo  \a \\ \" \$BEEP ;
echo "\a \\ \" \$BEEP";
echo '\a \\ \" \$BEEP';
echo 'beep\' 'boop'

